{
  "info": {
    "name": "Email Analyzer API",
    "schema": "https://schema.getpostman.com/json/collection/v2.1.0/collection.json"
  },
  "item": [
    {
      "name": "Health Check",
      "request": {
        "method": "GET",
        "header": [],
        "url": { "raw": "{{baseUrl}}/health", "host": ["{{baseUrl}}"], "path": ["health"] }
      }
    },
    {
      "name": "Analyze Header",
      "request": {
        "method": "POST",
        "header": [{"key": "Content-Type", "value": "application/json"}],
        "body": { "mode": "raw", "raw": "{\n    \"header\": \"Received: from smtp.gmail.com by example.com\"\n}" },
        "url": { "raw": "{{baseUrl}}/analyze", "host": ["{{baseUrl}}"], "path": ["analyze"] }
      }
    }
  ]
}
